{
  "short_name": "react-chrome-extension-template-side-panel",
  "name": "React Chrome Extension Template: Side Panel",
  "version": "0.2.0",
  "manifest_version": 3,
  "background": {
    "service_worker": "background.js",
    "type": "module"
  },
  "action": {
    "default_popup": ""
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>", "http://*/*", "https://*/*"],
      "css": ["dist/index.d5d6de48.css"],
      "js": ["dist/index.6099912e.js"]
    }
  ],
  "permissions": ["bookmarks", "storage", "scripting", "tabs", "activeTab"],
  "icons": {}
}
